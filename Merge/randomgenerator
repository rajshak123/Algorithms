import java.util.concurrent.ThreadLocalRandom;
public class randomgenerator {
    public static int[] random(){

        int[] arr=new int[100];
        for(int i=0;i<100;i++){arr[i]=ThreadLocalRandom.current().nextInt(50,1000000);}
        for (int i:arr
                ) {
            System.out.println(i);
        }
        return arr;
    }
}

